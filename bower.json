{
  "name": "Pyro",
  "version": "0.0.0",
  "homepage": "https://github.com/pyrolabs/Pyro",
  "authors": [
    "Scott Prue <prescottprue@gmail.com>"
  ],
  "license": "MIT",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "bootstrap-material-design": "~0.2.0",
    "underscore": "~1.7.0",
    "angular-tree-control": "~0.2.7",
    "bootstrap": "~3.3.1",
    "aws-sdk": "~2.1.1",
    "firebase": "~2.0.6",
    "angularfire": "~0.9.0",
    "angular-ui-router": "~0.2.13",
    "angular-ui-ace": "bower",
    "fontawesome": "~4.2.0",
    "ionic": "~0.9.27",
    "ionicons": "~2.0.0"
  },
  "overrides": {
    "ace-builds": {
      "main": [
        "src-noconflict/ace.js"
      ]
    },
    "ionic": {
      "main": []
    }
  },
  "install": {
    "base":"app/bower_libs",
    "path": {"css":"{name}/css","eot":"{name}/fonts","ttf": "{name}/fonts", "woff": "{name}/fonts", "svg":"{name}/fonts" },
    "sources": {
      "ace-builds": "bower_components/ace-builds/src-noconflict/ace.js",
      "ionicons": ["bower_components/ionicons/css/*.*","bower_components/ionicons/fonts/*.*"],
      "bootstrap": "bower_components/bootstrap/dist/css/bootstrap.min.css",
      "bootstrap-material-design":"bower_components/bootstrap-material-design/dist/css/*.*",
      "angular-tree-control":"bower_components/angular-tree-control/css/tree-control-attribute.css",
      "fontawesome":"bower_components/fontawesome/css/font-awesome.min.css"

    }
  }
}
